{
  "name": "normalizr",
  "version": "4.0.0",
  "description": "Normalizes and denormalizes JSON according to schema for Redux and Flux applications",
  "bugs": {
    "url": "https://github.com/paularmstrong/normalizr/issues"
  },
  "homepage": "https://github.com/paularmstrong/normalizr",
  "repository": {
    "url": "https://github.com/paularmstrong/normalizr.git",
    "type": "git"
  },
  "keywords": [
    "flux",
    "redux",
    "normalize",
    "denormalize",
    "api",
    "json",
    "typescript"
  ],
  "files": [
    "dist/"
  ],
  "type": "module",
  "main": "./dist/normalizr.cjs",
  "module": "./dist/normalizr.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/normalizr.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/normalizr.cjs"
      }
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "vite build",
    "clean": "rimraf dist",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint src --ext .ts --fix",
    "lint:ci": "eslint src --ext .ts",
    "prepublishOnly": "npm run build",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "author": "Nicholas Husher <nhusher@gmail.com>",
  "contributors": [
    "Paul Armstrong",
    "Dan Abramov"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@typescript-eslint/parser": "^8.19.1",
    "@vitest/coverage-v8": "^2.1.9",
    "eslint": "^9.17.0",
    "immutable": "^4.3.7",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.2",
    "vite": "^6.0.7",
    "vite-plugin-dts": "^4.4.0",
    "vitest": "^2.1.8"
  }
}
